		"lint-staged": "^10.5.4",
		"prettier": "^2.2.1",
		"ts-jest": "^26.5.3",
		"ts-node": "^9.1.1",
		"tsc-alias": "^1.2.7",
		"tsutils": "^3.21.0",
		"ttypescript": "^1.5.12",
		"typescript": "^4.2.3",
		"typescript-transform-paths": "^2.2.3",
		"web3": "^1.3.4"
